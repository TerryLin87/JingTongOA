
@{
    Layout = null;
    List<JianTongBLL.workFlowHistory> historyList = ViewBag.HistoryList as List<JianTongBLL.workFlowHistory>;
    JianTongBLL.WorkInfoClass workInfo = ViewBag.WorkInfo as JianTongBLL.WorkInfoClass;
    int Count = 1;
    string companyName = "", investName = "", productTypeName = "", cTypeName = "", productSourceName = "", vName = "";

    if (workInfo != null)
    {
        companyName = workInfo.companyName;
        JianTongBLL.InvestmentType investmentType = JianTongFun.EntityFun.CreateInstance.GetTByIntId<JianTongBLL.InvestmentType>(workInfo.investmentType);
        if (investmentType != null)
        {
            investName = investmentType.Name;
        }

        JianTongBLL.ProductType ProductType = JianTongFun.EntityFun.CreateInstance.GetTByIntId<JianTongBLL.ProductType>(workInfo.productType);
        if (ProductType != null)
        {
            productTypeName = ProductType.Name;
        }

        JianTongBLL.ConsultationType ConsultationType = JianTongFun.EntityFun.CreateInstance.GetTByIntId<JianTongBLL.ConsultationType>(workInfo.consultationType);
        if (ConsultationType != null)
        {
            cTypeName = ConsultationType.Name;
        }

        JianTongBLL.ProductSource ProductSource = JianTongFun.EntityFun.CreateInstance.GetTByIntId<JianTongBLL.ProductSource>(workInfo.productSource);
        if (ProductSource != null)
        {
            productSourceName = ProductSource.Name;
        }

        JianTongBLL.ValuationType ValuationType = JianTongFun.EntityFun.CreateInstance.GetTByIntId<JianTongBLL.ValuationType>(workInfo.valuationType);
        if (ValuationType != null)
        {
            vName = ValuationType.Name;
        }

    }
}

